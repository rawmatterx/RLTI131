# OpenRouter configuration for I131 Therapy Assistant
# Copy this file to .env.local and fill in values

# Required
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY
# Optional but recommended for OpenRouter attribution/policy
OPENROUTER_SITE_URL=http://localhost:3000
OPENROUTER_SITE_TITLE=I131 Therapy Assistant

# Model selection (default set in code)
OPENROUTER_MODEL=google/gemma-3-27b-it:free

# Supabase (required for /todos)
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
